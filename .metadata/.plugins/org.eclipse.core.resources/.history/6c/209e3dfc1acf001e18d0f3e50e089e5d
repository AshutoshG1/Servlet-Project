package p1;

import java.util.Arrays;
import java.util.Scanner;

public class B {
	public static void deciToBinary(int n) {
		int[] a = new int[20];
		int i=0;
		while(n>0) {
			a[i]=n%2;
			n=n/2;
			i++;
		}
		for(int j=i-1;j>=0;j--) {
			System.out.print(a[j]);
		}
	}
	public static void main(String[] args) {
		int n = 19;
		deciToBinary(n);
	}
}