package p1;

public class A {
	  A(){
		  System.out.println("default constructor ");
	  }
	  A(char x){
		  System.out.println("single argument constructor "+x);
	  }
	  A(String x,int y){
		  System.out.println("double arguments construcot "+x+y);
	  }
	  public static void main(String[] args) {
		A a1 = new A();
		A a2 = new A('c');
		A a3 = new A("string", 200);
	}
}       